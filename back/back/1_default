server {
    listen 2067;
    server_name 85.209.9.201;  # Или ваш IP адрес

    location = /favicon.ico { access_log off; log_not_found off; }
    location /static/ {
        root /var/www/pharmacy_back;  # Путь к каталогу с проектом Django
    }

    location / {
        include proxy_params;
        proxy_pass http://127.0.0.1:2067;  # Проксирование на порт, который вы указали в Gunicorn
        proxy_redirect off;
    }

    error_page 500 502 503 504 /500.html;
    location = /500.html {
        root /var/www/pharmacy_back;  # Путь к каталогу с проектом Django
    }
}
